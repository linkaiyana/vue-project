{
  "name": "vue-project",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite -- --",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "preview:dino2024": "vite preview --outDir dist/dino/2024/myActivity",
    "build-only": "vite build --emptyOutDir -- ",
    "type-check": "vue-tsc --build",
    "type-check:dino2024": "vue-tsc --noEmit -p dino/2024/myActivity/tsconfig.json",
    "type-check:dino2025": "vue-tsc --noEmit -p dino/2025/myActivity/tsconfig.json",
    "lint": "eslint . --fix --cache",
    "format": "prettier --write --experimental-cli src/"
  },
  "dependencies": {
    "pinia": "^3.0.4",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.3.0",
    "@tsconfig/node24": "^24.0.3",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "~10.5.1",
    "jiti": "^2.6.1",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "tslib": "^2.8.1",
    "typescript": "~5.9.0",
    "vite": "^7.2.4",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vue-tsc": "^3.1.5"
  }
}